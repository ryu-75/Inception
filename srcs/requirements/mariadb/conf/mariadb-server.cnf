[mysqld]
user=mysql
datadir=/var/lib/mysql
tmpdir=/tmp
basedir=/usr
port = 3306
pid-file=/run/mysqld/mysqld.pid
socket=/run/mysqld/mysqld.sock
bind-address=0.0.0.0
log-bin =/run/mysqld/mysql-bin
log-basename=mysqld
lc-messages-dir=/usr/share/mysql
lc-messages=en_US
skip-networking=false
skip-name-resolve
general-log
log-slow-queries
